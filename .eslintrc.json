{
	"root": true,
	"env": {
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"project": "./tsconfig.json"
	},
	"plugins": [
		"@typescript-eslint",
		"no-autofix"
	],
	"parser": "@typescript-eslint/parser",
	"extends": [
		"plugin:@typescript-eslint/recommended"
	],
	"rules": {
		"no-autofix/prefer-const": 1,

		"@typescript-eslint/semi": 1,
		"@typescript-eslint/adjacent-overload-signatures": 1,
		"@typescript-eslint/array-type": [1, {
			"default": "array"
		}],
		"@typescript-eslint/await-thenable": 0,
		"@typescript-eslint/ban-ts-ignore": 0,
		"@typescript-eslint/explicit-module-boundary-types": 0,
		"@typescript-eslint/ban-types": 1,
		"@typescript-eslint/ban-ts-comment": 0,
		"@typescript-eslint/consistent-type-definitions": [1, "interface"],
		"@typescript-eslint/explicit-function-return-type": 0,
		"@typescript-eslint/explicit-member-accessibility": 0,
		"@typescript-eslint/func-call-spacing": 1,
		"@typescript-eslint/generic-type-naming": 0,
		"@typescript-eslint/indent": [1, "tab", {
			"SwitchCase": 1
		}],
		"@typescript-eslint/interface-name-prefix": [0, "always"],
		"@typescript-eslint/member-delimiter-style": [1, {
			"multiline": {
				"delimiter": "semi",
				"requireLast": true
			},
			"singleline": {
				"delimiter": "semi",
				"requireLast": false
			}
		}],
		"@typescript-eslint/member-naming": [0, {
			"private": "^_"
		}],
		"@typescript-eslint/member-ordering": 0,
		"@typescript-eslint/no-array-constructor": 1,
		"@typescript-eslint/no-empty-function": 0,
		"@typescript-eslint/no-empty-interface": 0,
		"@typescript-eslint/no-explicit-any": 0,
		"@typescript-eslint/no-extra-parens": 1,
		"@typescript-eslint/no-extraneous-class": 0,
		"@typescript-eslint/no-floating-promises": 0,
		"@typescript-eslint/no-for-in-array": 1,
		"@typescript-eslint/no-inferrable-types": 1,
		"@typescript-eslint/no-magic-numbers": [0, {
			"ignoreEnums": true
		}],
		"@typescript-eslint/no-misused-new": 1,
		"@typescript-eslint/no-misused-promises": 0,
		"@typescript-eslint/no-namespace": 1,
		"@typescript-eslint/no-non-null-assertion": 0,
		"@typescript-eslint/no-parameter-properties": 0,
		"@typescript-eslint/no-require-imports": 1,
		"@typescript-eslint/no-type-alias": 0,
		"@typescript-eslint/no-unnecessary-qualifier": 1,
		"@typescript-eslint/no-unnecessary-type-assertion": 1,
		"@typescript-eslint/no-unused-vars": 0,
		"@typescript-eslint/no-use-before-define": 0,
		"@typescript-eslint/no-useless-constructor": 1,
		"@typescript-eslint/no-var-requires": 1,
		"@typescript-eslint/prefer-for-of": 1,
		"@typescript-eslint/prefer-includes": 1,
		"@typescript-eslint/prefer-readonly": 1,
		"@typescript-eslint/prefer-regexp-exec": 1,
		"@typescript-eslint/prefer-string-starts-ends-with": 0,
		"@typescript-eslint/promise-function-async": 1,
		"@typescript-eslint/require-array-sort-compare": 1,
		"@typescript-eslint/require-await": 1,
		"@typescript-eslint/restrict-plus-operands": 1,
		"@typescript-eslint/strict-boolean-expressions": 0,
		"@typescript-eslint/type-annotation-spacing": [1, {
			"before": false,
			"after": true
		}],
		"@typescript-eslint/unbound-method": 0,
		"@typescript-eslint/unified-signatures": 1,

		"valid-jsdoc": 0,
		"camelcase": 1,
		"comma-dangle": 0,
		"@typescript-eslint/comma-dangle": [1, {
			"arrays": "always-multiline",
			"objects": "always-multiline",
			"imports": "always-multiline",
			"exports": "never",
			"functions": "always-multiline",
			"enums": "always"
		}],
		"no-alert": 0,
		"curly": 0,
		"no-var": 1,
		"no-undef": 1,
		"no-cond-assign": 1,
		"no-constant-condition": 1,
		"no-dupe-keys": 1,
		"no-duplicate-case": 1,
		"no-empty": 0,
		"no-extra-boolean-cast": 1,
		"no-extra-parens": 0,
		"no-extra-semi": 1,
		"no-func-assign": 1,
		"no-inner-declarations": 1,
		"no-invalid-regexp": 1,
		"no-unsafe-negation": 1,
		"no-regex-spaces": 1,
		"no-redeclare": 1,
		"spaced-comment": [1, "always", { "exceptions": ["/"] }],
		"no-sparse-arrays": 1,
		"no-unexpected-multiline": 1,
		"no-unreachable": 0,
		"use-isnan": 1,
		"valid-typeof": [1, { "requireStringLiterals": true }],
		"array-callback-return": 1,
		"block-scoped-var": 1,
		"complexity": [0, 10],
		"consistent-return": 1,
		"default-case": 0,
		"dot-notation": 1,
		"eqeqeq": [
			1,
			"always",
			{
				"null": "ignore"
			}
		],
		"guard-for-in": 0,
		"no-case-declarations": 1,
		"no-else-return": 0,
		"no-empty-function": 0,
		"no-empty-pattern": 1,
		"no-eq-null": 0,
		"no-extend-native": 1,
		"no-extra-bind": 1,
		"no-extra-label": 1,
		"no-fallthrough": 1,
		"no-implied-eval": 1,
		"no-loop-func": 1,
		"no-multi-spaces": 1,
		"no-new-func": 1,
		"no-global-assign": 1,
		"no-new-wrappers": 1,
		"no-proto": 1,
		"no-return-assign": 1,
		"no-script-url": 1,
		"no-self-assign": 1,
		"no-param-reassign": [
			0,
			{
				"props": true
			}
		],
		"no-self-compare": 1,
		"no-sequences": 1,
		"no-throw-literal": 1,
		"no-unmodified-loop-condition": 1,
		"no-unused-expressions": 0,
		"no-unused-labels": 1,
		"no-useless-call": 1,
		"no-useless-concat": 1,
		"no-useless-escape": 1,
		"no-void": 1,
		"no-warning-comments": 0,
		"radix": 1,
		"wrap-iife": 1,
		"yoda": 1,
		"no-label-var": 1,
		"no-shadow": 0,
		"@typescript-eslint/no-shadow": 1,
		"newline-before-return": 0,
		"newline-after-var": 0,
		"array-bracket-spacing": [
			1,
			"never"
		],
		"block-spacing": [
			1,
			"always"
		],
		"brace-style": 1,
		"comma-spacing": [
			1,
			{
				"before": false,
				"after": true
			}
		],
		"comma-style": [
			1,
			"last"
		],
		"computed-property-spacing": [
			1,
			"never"
		],
		"eol-last": 1,
		"key-spacing": [
			1,
			{
				"singleLine": {
					"beforeColon": false,
					"afterColon": true
				},
				"multiLine": {
					"beforeColon": false,
					"afterColon": true
				}
			}
		],
		"keyword-spacing": [
			1,
			{
				"before": true,
				"after": true
			}
		],
		"linebreak-style": [
			1,
			"unix"
		],
		"max-depth": [
			0,
			4
		],
		"new-cap": [1, { "capIsNewExceptions": ["Component"] }],
		"new-parens": 1,
		"newline-per-chained-call": [0, { "ignoreChainWithDepth": 2 }],
		"no-nested-ternary": 0,
		"no-new-object": 1,
		"no-array-constructor": 1,
		"object-shorthand": 1,
		"object-curly-spacing": [
			1,
			"always"
		],
		"quote-props": [
			1,
			"as-needed"
		],
		"object-property-newline": 0,
		"operator-assignment": [
			1,
			"always"
		],
		"operator-linebreak": [
			1,
			"after"
		],
		"quotes": [
			1,
			"single",
			{
				"allowTemplateLiterals": true
			}
		],
		"space-before-blocks": 1,
		"space-before-function-paren": [1, {
			"anonymous": "never",
			"named": "never",
			"asyncArrow": "always"
		}],
		"space-in-parens": [
			1,
			"never"
		],
		"space-infix-ops": 1,
		"space-unary-ops": 1,
		"padded-blocks": [
			1,
			"never"
		],
		"unicode-bom": [
			1,
			"never"
		],
		"no-unneeded-ternary": 1,
		"no-underscore-dangle": 0,

		"no-new-symbol": 1,
		"no-confusing-arrow": 0,
		"no-this-before-super": 1,
		"no-useless-computed-key": 1,
		"no-useless-constructor": 0,
		"no-dupe-class-members": 1,
		"no-duplicate-imports": 0,
		"@typescript-eslint/no-duplicate-imports": ["warn"],
		"no-iterator": 1,

		"prefer-numeric-literals": 1,
		"template-curly-spacing": [1, "never"],
		"one-var-declaration-per-line": [
			1,
			"always"
		],

		"no-magic-numbers": [
			0,
			{
				"ignore": [
					0,
					1,
					100
				],
				"ignoreArrayIndexes": true
			}
		],
		"max-len": [
			0,
			100
		],
		"no-unused-vars": 0,
		"func-names": 0,


		"prefer-const": 0,
		"prefer-spread": 1,
		"prefer-destructuring": 0,
		"prefer-rest-params": 1,
		"prefer-template": 1,
		"prefer-arrow-callback": 1,
		"arrow-spacing": [1, { "before": true, "after": true }],
		"arrow-parens": [1, "as-needed", { "requireForBlockBody": false }],
		"arrow-body-style": [1, "as-needed"],
		"constructor-super": 1,
		"no-class-assign": 1,
		"no-const-assign": 1,
		"no-useless-rename": 1,
		"require-yield": 1,
		"sort-imports": 0
	}
}
